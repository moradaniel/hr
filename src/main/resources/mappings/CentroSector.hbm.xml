<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
       "-//Hibernate/Hibernate Mapping DTD//EN"
       "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.dpi.centroSector.CentroSectorImpl"    table="CENTROSECTOR">

    <id name="id" column="ID" type="long">  
          <generator class="sequence">   
				<param name="sequence">CENTROSECTOR_SEQ</param>
          </generator>  
        </id> 
    
    <property name="codigoCentro" column="CODIGOCENTRO" type="string"/>
    <property name="codigoSector" column="CODIGOSECTOR" type="string"/>
        
    <property name="nombreCentro" column="NOMBRECENTRO" type="string"/> 
    <property name="nombreSector" column="NOMBRESECTOR" type="string"/>
    
    <!-- muchos centros sectores pueden tener 1 reparticion -->
    <many-to-one
			name="reparticion"
			class="org.dpi.reparticion.ReparticionImpl"
			column="REPARTICIONID"
			not-null="false"
			lazy="proxy"
			cascade="none"
			foreign-key="fk_ReparticionCentroSector__Reparticion"
		>
			<meta attribute="use-in-tostring">true</meta>
			<meta attribute="use-in-equals">true</meta>
		</many-to-one>

  </class>
</hibernate-mapping>